{
    "name": "reham.hesham/design-patterns",
    "description": "code examples for design pattern in php",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Creational\\": "Creational/",
            "Strctural\\": "Structural/",
            "Behavioural\\": "Behavioural/",
            "Others\\": "Others"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests/"
        }
    },
    "authors": [
        {
            "name": "Reham Hesham"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "repositories": [
        {
            "type": "path",
            "url": "BasicAuthLib"
        },
        {
            "type": "path",
            "url": "TokenAuthLib"
        }
    ],
    "require": {
        "reham/basic-auth-lib": "dev-master",
        "reham/token-auth-lib": "dev-master"
    },
    "license": ["MIT"]
}
